Data] = useState<any>([])\n   const historyTasksHandle = () => {\n      historyTasks({}).then(data => {\n         setData(data)\n      })\n   }\n   useEffect(() => {\n      historyTasksHandle()\n   }, [])\n   const returnIconType = (status: string) => {\n      if (['FAIL', 'REFUSED'].includes(status)) {\n         return \"fail\"\n      }\n      if ('SUCCESS' === status) {\n         return \"success\"\n      }\n      return \"waiting\"\n   }\n   return (\n      <ul className=\"task-wrap\">\n         {\n            data.length > 0 ? data.map((item: any) => {\n               return (\n                  <li className=\"timers-item\" key={item.taskId}>\n                     <p className=\"order-number\">订单号:{item.taskId}</p>\n                     <div className=\"task-bottom\">\n                        <div className=\"left\">\n                           <i className={item.bankType === 'ALIPAY' ? \"ali-pay-icon\" : \"def-bank-icon\"}></i>\n                        </div>\n                        <div className=\"min\">\n                           <span>¥{toFixed(item.cashAmount)}</span>\n                           <span>任务奖励:<i></i>{toFixed(item.taskBonus)}</span>\n                           <span>{mm_dd_hh_mm_ss2(item.userLockTimeout)}</span>\n                        </div>\n                        <div className=\"right\">\n                           <span className={returnIconType(item.status)}></span>\n                           <span>{status[item.status]}</span>\n                        </div>\n                     </div>\n                  </li>\n               )\n            }) : <li className=\"no-task-detail\">暂无记录数据</li>\n         }\n      </ul>\n   )\n}\nexport default OffTimer;","export default __webpack_public_path__ + \"static/media/order_done.7cafcd73.mp3\";","export const checkSound = () => {\n let hasSound = localStorage.getItem('sound') || true;\n return hasSound === 'false' ? false : true\n}","import React, { useState, useEffect, useRef } from \"react\"