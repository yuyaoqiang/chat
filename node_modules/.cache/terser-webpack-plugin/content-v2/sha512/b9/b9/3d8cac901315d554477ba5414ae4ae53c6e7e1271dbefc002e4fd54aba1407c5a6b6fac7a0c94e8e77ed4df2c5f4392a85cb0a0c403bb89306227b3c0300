 比赛结果大小类型\nexport enum matchBallType {\n big = 1, //大\n small, // 小\n walk, // 走水\n}\n// 推送类型\nexport enum pusherType {\n mainPush = 1, // 主推\n minorPush = 2 // 次推\n}","import dayjs from \"dayjs\"\n\nexport const yyyymmddhhmmss = (date: string) => {\n return dayjs(date).format(\"YYYY-MM-DD HH:mm:ss\")\n}\nexport const mm_dd_hh_mm_ss2 = (date: string) => {\n if (date) {\n  return dayjs(date).format(\"MM-DD HH:mm:ss\")\n }\n}\nexport const hh_mm_ss_zh = (date: string) => {\n let start = dayjs().valueOf();\n let end = dayjs(date).valueOf();\n let result = end - start;\n let hour = dayjs(result).hour();\n let minute = dayjs(result).minute();\n let second = dayjs(result).second();\n return `${hour}小时${minute}分${second}秒`\n}\n\nexport const seconds = (date: string) => {\n let start = dayjs();\n let end = dayjs(date);\n let diff = end.diff(start, \"seconds\")\n return diff\n}\nexport const mm_ss_zh = (seconds: number) => {\n return dayjs(seconds * 1000).format(\"mm分ss秒\")\n}","import React from \"react\"\nimport { matchType, matchBallType } from \"../../config\"\nimport { yyyymmddhhmmss } from \"@utils/dataTime\"\nimport \"./style.scss\"\n\nconst Match = (props: MatchProps) => {\n const { data, changeCheck } = props;\n\n // 胜负平\n const click = (key: number) => {\n  const copy: any = Object.assign({}, data);\n  const { firstRecommend, secondRecommend } = copy;\n  if (!firstRecommend) {\n   copy.firstRecommend = key\n   changeCheck(copy)\n   return;\n  }\n  if (firstRecommend === key && secondRecommend) {\n   return;\n  }\n  if (firstRecommend === key) {\n   copy.firstRecommend = undefined\n   changeCheck(copy)\n   return;\n  }\n  if (secondRecommend === key) {\n   copy.secondRecommend = undefined;\n   changeCheck(copy)\n   return;\n  }\n  copy.secondRecommend = key;\n  changeCheck(copy)\n }\n\n // 大小球\n const clickBall = (key: number) => {\n  const copy: any = Object.assign({}, data);\n  const { sizeBallRecommend, secondSizeBallRecommend } = copy;\n  changeCheck(copy)\n  if (!sizeBallRecommend) {\n   copy.sizeBallRecommend = key\n   return;\n  }\n  if (sizeBallRecommend === key && secondSizeBallRecommend) {\n   return;\n  }\n  if (sizeBallRecommend === key) {\n   copy.sizeBallRecommend = undefined\n   return;\n  }\n  if (secondSizeBallRecommend === key) {\n   copy.secondSizeBallRecommend = undefined;\n   return;\n  }\n  copy.secondSizeBallRecommend = key;\n }\n\n\n const getActivity = (data: MatchData, type: number) => {\n  const { firstRecommend, secondRecommend } = data;\n  if (firstRecommend === type) {\n   return \"main-activity\"\n  }\n  if (secondRecommend === type) {\n   return \"minor-activity\"\n  }\n  return \"\"\n }\n\n const getActivityBall = (data: MatchData, type: number) => {\n  const { sizeBallRecommend, secondSizeBallRecommend } = data;\n  if (sizeBallRecommend === type) {\n   return \"main-activity\"\n  }\n  if (secondSizeBallRecommend === type) {\n   return \"minor-activity\"\n  }\n  return \"\"\n }\n\n // 清空\n const clearClick = () => {\n  const copy = Object.assign({}, data);\n  copy.firstRecommend = undefined;\n  copy.secondRecommend = undefined;\n  copy.sizeBallRecommend = undefined;\n  copy.secondSizeBallRecommend = undefined;\n  changeCheck(copy)\n }\n return (\n  <div className=\"push-match\">\n   <div className=\"left\">\n    <p className=\"home-name\">\n     {data.homeTeam}\n    </p>\n    <p className=\"match-info\">\n     <span>{data.matchName}</span>\n     <span>{yyyymmddhhmmss(data.matchStartTime)}</span>\n    </p>\n   </div>\n   <div className=\"center\">\n    <p className=\"vs\">VS</p>\n    <p className=\"win-loss\">\n     <span className={getActivity(data, matchType.homeWin)} onClick={() => click(matchType.homeWin)}>主胜</span>\n     <span className={getActivity(data, matchType.draw)} onClick={() => click(matchType.draw)}>平局</span>\n     <span className={getActivity(data, matchType.awayWin)} onClick={() => click(matchType.awayWin)}>客胜</span>\n    </p>\n    <p className=\"big-small\">\n     <span classNa