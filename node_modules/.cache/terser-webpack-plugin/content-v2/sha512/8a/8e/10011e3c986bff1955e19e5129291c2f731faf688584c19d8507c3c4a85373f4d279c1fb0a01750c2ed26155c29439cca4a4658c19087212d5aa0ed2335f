{ yyyymmddhhmmss } from \"@utils/dataTime\"\nimport \"./style.scss\"\n\nconst Match = (props: MatchProps) => {\n const { data, changeCheck } = props;\n\n // 胜负平\n const click = (key: number) => {\n  const copy: any = Object.assign({}, data);\n  const { firstRecommend, secondRecommend } = copy;\n  if (!firstRecommend) {\n   copy.firstRecommend = key\n   changeCheck(copy)\n   return;\n  }\n  if (firstRecommend === key && secondRecommend) {\n   return;\n  }\n  if (firstRecommend === key) {\n   copy.firstRecommend = undefined\n   changeCheck(copy)\n   return;\n  }\n  if (secondRecommend === key) {\n   copy.secondRecommend = undefined;\n   changeCheck(copy)\n   return;\n  }\n  copy.secondRecommend = key;\n  changeCheck(copy)\n }\n\n // 大小球\n const clickBall = (key: number) => {\n  const copy: any = Object.assign({}, data);\n  const { sizeBallRecommend, secondSizeBallRecommend } = copy;\n  changeCheck(copy)\n  if (!sizeBallRecommend) {\n   copy.sizeBallRecommend = key\n   return;\n  }\n  if (sizeBallRecommend === key && secondSizeBallRecommend) {\n   return;\n  }\n  if (sizeBallRecommend === key) {\n   copy.sizeBallRecommend = undefined\n   return;\n  }\n  if (secondSizeBallRecommend === key) {\n   copy.secondSizeBallRecommend = undefined;\n   return;\n  }\n  copy.secondSizeBallRecommend = key;\n }\n\n\n const getActivity = (data: MatchData, type: number) => {\n  const { firstRecommend, secondRecommend } = data;\n  if (firstRecommend === type) {\n   return \"main-activity\"\n  }\n  if (secondRecommend === type) {\n   return \"minor-activity\"\n  }\n  return \"\"\n }\n\n const getActivityBall = (data: MatchData, type: number) => {\n  const { sizeBallRecommend, secondSizeBallRecommend } = data;\n  if (sizeBallRecommend === type) {\n   return \"main-activity\"\n  }\n  if (secondSizeBallRecommend === type) {\n   return \"minor-activity\"\n  }\n  return \"\"\n }\n\n // 清空\n const clearClick = () => {\n  const copy = Object.assign({}, data);\n  copy.firstRecommend = undefined;\